<%= render partial: 'shared/breadcrumbs', locals: { page_title: "Alumni Signup" } %>

<div class="application">
<h1>Alumni Info</h1>
  <%= bootstrap_form_for @alumnu, :url => {:action => "create"} do |f| %>
    <p><%= f.text_field :home_phone %></p>
    <p><%= f.text_field :mobile_phone %></p>
    <p><%= f.text_field :work_phone %></p>
    <p><%= f.text_field :street %></p>
    <p><%= f.text_field :city %></p>
    <p><%= f.text_field :state %></p>
    <p><%= f.text_field :zip_code %></p>
    <p><%= f.text_field :country %></p>
    <p><%= f.date_field :birth_date %></p>
    <p><%= f.text_field :graduation_year %></p>
    <p><%= f.text_field :occupation %></p>
    <p><%= f.text_area :bio %></p>
    <p><%= f.submit :Save %></p>
  <% end %>
</div>

