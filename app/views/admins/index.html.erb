<div class="adminsIndexContainer">
  <h3>Admin Users</h3>
</div>
<div class="users-table">
  <table>
    <tr>
      <th>Email</th>
      <th>Full Name</th>
      <th>Remove Admin</th>
    </tr>

    <% @admins.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.first_name + ' ' + user.last_name %></td>
        <td><%= link_to "Delete", admin_path(user.id), :data => {:confirm => "Do you want to delete #{user.first_name}?"}, :method => :delete %></td>
      </tr>
  	<% end %>
  </table>
</div>
